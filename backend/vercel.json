{
    "version": 2,
    "builds": [
    { "src": "src/app.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "src/app.js" },
    { "src": "/(.*)", "dest": "src/app.js" }  ],
    "methods":["GET","PUT","POST","DELETE"],
    "headers":{
      "Access-Control-Allow-Origin":"*"
    }
}
